#Let's make it 

i = 1
while i < 5:
    if i == 1:
        print('💗', end="")
        print('🖤' * 30)
        i += 1
    if i == 2:
              print('💗' * i, end="")
              print('🖤' * 30)
              i += 1
    if i == 3:
              print('💗' * i, end="")
              print('🖤' * 30)
              i += 1
    if i == 4:
              print('💗' * i, end="")
              print('🤍' * 30)
              i += 1
    if i == 5:
              print('💗' * i, end="")
              print('🤍' * 30)
              i += 1
while i >=1:
    if i == 6:
        print('💗' * i, end="")
        print('🤍' * 30)
        i -= 1
    if i == 5:
        print('💗' * i, end="")
        print('🤍' * 30)
        i -= 1
    if i == 4:
        print('💗' * i, end="")
        print('🤍' * 30)
        i -= 1
    if i == 3:
        print('💗' * i, end="")
        print('💚' * 30)
        i -= 1
    if i == 2:
        print('💗' * i, end="")
        print('💚' * 30)
        i -= 1
    if i == 1:
        print('💗' * i, end="")
        print('💚' * 30)
        i -= 1
        
    
    
    
